/* Filename: lab12_1
   Description: Tablica 2-wymiarowa
*/

#include <stdio.h>

int main()
{
    int tab[2][3]={{1,2,3},{4,5,6}}; //wpisywanie "z reki"
    int i,j; //liczniki do petli for
    
    printf("WYPISYWANIE\n");
      for(i=0;i<2;i++){
        for(j=0;j<3;j++){
            printf("%d\n",tab[i][j]);
        }
    }
	printf("WYPISYWANIE elementu [1][2]:\n");
	printf("%d\n",tab[1][2]);
    return 0;
}
